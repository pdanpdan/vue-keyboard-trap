{
  "name": "@pdanpdan/vue-keyboard-trap",
  "version": "1.0.19",
  "description": "Vue3 and Vue2 directive for keyboard navigation - roving movement and trapping inside container",
  "productName": "Vue Keyboard Trap",
  "author": {
    "name": "Dan Popescu",
    "email": "pdan.popescu@gmail.com",
