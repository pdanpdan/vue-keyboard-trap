    "type": "github",
    "url": "https://github.com/sponsors/pdanpdan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdanpdan/vue-keyboard-trap.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "directive",
    "keyboard",
    "navigation",
    "trap",
    "roving",
    "grid",
    "gridcell",
    "a11y",
    "accessibility",
    "cycle",
    "tab",
    "tabindex",
    "wai-aria"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pdanpdan/vue-keyboard-trap/issues"
  },
